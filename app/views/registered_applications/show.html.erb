<h1><%= @registered_appication.name %></h1>
<div class="container-fluid">
  <div class="col-md-8">
    <p class="lead"><%= @registered_appication.name %></p>
    <small><%= @registered_appication.url %></small>
      <div class="col-md-4">
        <%= button_to "Add an Application", new_registered_application_path, class: 'btn btn-default glyphicon glyphicon-pencil' %>
          <span class="glyphicon: glyphicon glyphicon-pencil" aria-hidden="true"></span>
        <%= button_to "Delete an Application", method: :delete, class: 'btn btn-default glyphicon glyphicon-trash' %>
          <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
    </div>
  </div>
</div>
